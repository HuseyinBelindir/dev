    room.setTeamColors(1, 60, 0xFFFFFF, [0xFF0800, 0xFF0800, 0xFF0800]);
    room.setTeamColors(2, 180, 0xFFFFFF, [0x0082E1, 0x0C161E, 0x0082E1]);
    SendLink(url)
}

